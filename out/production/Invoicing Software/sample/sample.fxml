<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="768.0" prefWidth="1024.0" GridPane.columnIndex="1">
         <children>
            <TabPane fx:id="tabPane" layoutY="100.0" prefHeight="668.0" prefWidth="1024.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab fx:id="tabbuttonDashboard" text="Dashboard">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                  </content>
                </Tab>
                <Tab fx:id="tabbuttonCustomers" text="Customers">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <TableView layoutX="14.0" layoutY="64.0" prefHeight="560.0" prefWidth="994.0">
                                <columns>
                                  <TableColumn prefWidth="52.0" text="Select" />
                                  <TableColumn prefWidth="168.0" text="Name" />
                                    <TableColumn minWidth="0.0" prefWidth="189.0" text="Company Name" />
                                    <TableColumn minWidth="0.0" prefWidth="157.0" text="Email" />
                                    <TableColumn prefWidth="126.0" text="Work Phone" />
                                    <TableColumn prefWidth="151.0" text="Receivables" />
                                    <TableColumn prefWidth="150.0" text="Unused Credits" />
                                </columns>
                              </TableView>
                              <Button layoutX="899.0" layoutY="21.0" mnemonicParsing="false" text="New Customer" />
                           </children></AnchorPane>
                  </content>
                </Tab>
                  <Tab fx:id="tabbuttonItems" text="Items List">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
                  <Tab fx:id="tabbuttonInvoices" text="Invoices">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
                  <Tab fx:id="tabbuttonPayments" text="Payments Received">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
                  <Tab fx:id="tabbuttonExpenses" text="Expenses">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
              </tabs>
            </TabPane>
         </children>
      </Pane>
   </children>
</GridPane>
